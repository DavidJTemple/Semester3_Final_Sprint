<html lang="en">
    <%- include('./partials/header.ejs') %>
    <body>
        <%- include('./partials/nav.ejs') %>
        <div class="lost content">
          <h2>Mongo Database</h2>
          <div class="dropdown">
            <button class="dropbtn">Find Name</button>
            <div class="dropdown-content">
              <% if (allCensus.length > 0) { %> <% allCensus.forEach(census => { %>
                  <p class="census">
                    <a href="<%= census.family_name%>"><b><%= census.family_name%>,</b>
                      <%=census.parent1_first_name%> <br/>
                      <span>Household ID:</span> <%=census.household_id%></a>
                  </p>
                  <% }) %> <% } %>
            </div>
        </div>
        <br></br>
        <div> <h2>Please Logout When Finished</h2>
          <form action="/logout?_method=DELETE" method="POST">
            <button id="gin" type="submit">Logout</button>
          </form>
        </div>
    </body>
    <%- include('./partials/footer.ejs') %>
</html>



    